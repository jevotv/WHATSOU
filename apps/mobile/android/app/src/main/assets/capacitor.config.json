{
	"appId": "com.whatsou.app",
	"appName": "WhatSou",
	"webDir": "out",
	"server": {
		"androidScheme": "https"
	},
	"plugins": {
		"Camera": {
			"permissions": [
				"camera",
				"photos"
			],
			"androidPhotoPickerPreferred": true
		},
		"Geolocation": {
			"permissions": [
				"location"
			]
		},
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		},
		"CapacitorHttp": {
			"enabled": false
		}
	}
}
